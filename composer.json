{
  "name": "Wing/Collector",
  "type": "library",
  "description": "基于mysql数据库binlog的增量订阅&消费",
  "keywords": ["mysql", "binlog", "php", "中间件", "数据监控"],
  "homepage": "http://www.itdfy.com/",
  "support": {
    "issues": "https://www.itdfy.com/",
    "email":"297341015@qq.com"
  },
  "authors": [
    {
      "name": "yuyi",
      "email": "297341015@qq.com",
      "homepage": "http://www.itdfy.com/"
    }
  ],
  "repositories":[
    {
      "type":"vcs",
      "url":"https://github.com/jilieryuyi/wing-file-system.git"
    },
    {
      "type": "composer",
      "url": "http://packagist.phpcomposer.com"
    },
    {
      "packagist": false
    }
  ],
  "config": {
    "preferred-install": "dist",
    "secure-http": false
  },
  "require": {
    "php": ">=5.0",
    "symfony/console":"~3.1",
    "wing/FileSystem":"dev-master",
    "psr/log": "~1.0",
    "php-amqplib/php-amqplib": ">=2.6",
    "workerman/workerman":"dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.0"
  },
  "autoload": {
    "psr-4": {"Wing\\": "src/"},
    "classmap": ["tests/unit/"],
    "files": [
      "src/helpers.php"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
